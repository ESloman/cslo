
println("Should be less than 5");
var x = 5;
if (x < 10) {
    println("less than 10!");
} else {
    println("10 or more!");
}

println("Print up to 20");
while (x < 20) {
    x = x + 1;
    println(x);
}

println("Shoud be more than 10");
if (x < 10) {
    println("less than 10!");
} else {
    println("10 or more!");
}

println("Decrementing");
while (x > 5) {
    x = x - 1;
    println(x);
}

println("Inline for");
for (var i = 0; i < 2; i++) { println(i); }

println("Using global x");
for (; x < 20; x++) {
    println(x);
}
