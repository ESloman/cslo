
for (var x = 0; x < 10; x++) {
    if (x == 2) {
        println("x is 2");
    } elif (x == 1) {
        println("x is 1");
    } elif (x == 3) {
        println("x is 3");
    } else {
        println("x is neither");
    }
}


var y = 0;
while (y < 10) {
    if (y == 0) {
        println("y is 0");
        println(y);
    } elif (y % 2 == 0) {
        println("y is even");
        println(y);
    } else {
        println("y is odd");
    }
    y += 1;
}

