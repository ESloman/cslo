
import math;

# ceil and floor
print("Printing ceil and floor stuff");
for (var x = 0; x < 10; x = x + 1.1) {
    print("Testing with ", x, "; ceil: ", math.ceil(x), ", floor: ", math.floor(x));
}

# sqrt
print(math.sqrt(9));  # 3

# sin
print(math.sin(1));

# cos
print(math.cos(1));

# tan
print(math.tan(1));
