
class TestClass {
    __init__(name, age) {
        this.name = name;
        this.age = age;
    }

    hello() {
        print("My name is: " + this.name);
    }
}

var test = TestClass("elliot", 29);
test.hello();
print(test.age);

var test2 = TestClass("bob", 43);
test2.hello();
print(test2.age);
