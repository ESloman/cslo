
class TestClass {
    func __init__(name, age) {
        self.name = name;
        self.age = age;
    }

    func hello() {
        print("My name is: " + self.name);
    }
}

var test = TestClass("elliot", 29);
var h = test.hello;
print(h);
h();
print(test);
print(test.name);
print(test.age);
print(test.hello);
test.hello();

var test2 = TestClass("bob", 43);
test2.hello();
